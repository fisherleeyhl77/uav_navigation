<launch>

  <!-- This file makes it easy to include the settings for sensor managers -->  

  <!-- Params for the octomap monitor -->
  <!--  <param name="octomap_frame" type="string" value="some frame in which the robot moves" /> -->
  <param name="octomap_frame" type="string" value="/rtabmap/odom" />  <!-- was world -->
  <param name="octomap_resolution" type="double" value="0.05" />
  <param name="max_range" type="double" value="3.5" />
  <param name="min_range" type="double" value="0.5" />



  <!-- Load the robot specific sensor manager; this sets the moveit_sensor_manager ROS parameter -->
  <arg name="moveit_sensor_manager" default="firefly_bot" />
  <include file="$(find rotors_moveit)/launch/$(arg moveit_sensor_manager)_moveit_sensor_manager.launch.xml" />
  
</launch>
